{
  "mappings": [
    {
      "request": {
        "method": "POST",
        "url": "/users",
        "headers": {
          "Content-Type": {
            "contains": "application/json"
          }
        },
        "bodyPatterns": [
          {
            "equalToJson": "{\n  \"userId\": \"${json-unit.any-string}\"}",
            "ignoreArrayOrder": true,
            "ignoreExtraElements": true
          }
        ]
      },
      "response": {
        "status": 204,
        "headers": {
          "Content-Type": "application/json"
        },
        "jsonBody": {
          "userId": 110,
          "id": 101
        }
      }
    }
  ]
}